<div class="container">

<h2>Edit articles</h2>

<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

<%= link_to 'Show', @article %> |
<%= link_to 'Back', articles_path %>
</div>
